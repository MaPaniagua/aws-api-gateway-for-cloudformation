{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "title": "ApiImport Request",
    "type": "object",
    "properties": {
        "failOnWarnings": {
            "type": "string",
            "pattern": "^(true|false)$",
            "default": "false"
        },
        "apiDefinition": {
            "type": "object"
        },
        "apiDefinitionS3Location": {
            "type": "object",
            "properties": {
                "bucket": {
                    "type": "string",
                    "required": true,
                    "description": "Bucket where the Swagger file is stored"
                },
                "key": {
                    "type": "string",
                    "required": true,
                    "description": "Swagger file object key"
                },
                "etag": {
                    "type": "string",
                    "description": "The S3 ETag of the Swagger file. If provided, a checksum validation will be performed"
                },
                "version": {
                    "type": "string",
                    "description": "If the Bucket is versioned you can point to a specific object version here. If omitted, the latest will be used."
                }
            }
        },
        "parameters": {
            "type": "object"
        },
        "restApiId": {
            "type": "string"
        },
        "lastModified": {
            "type": "string"
        }
    },
    "oneOf": [
        { "title": "apiDefinition", "required": ["apiDefinition"] },
        { "title": "apiDefinitionS3Location", "required": ["apiDefinitionS3Location"] }
    ],
    "additionalProperties": false
}
